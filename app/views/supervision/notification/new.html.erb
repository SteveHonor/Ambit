<h>Fiscalização | Notificação<div class="pull-right">nº notificação: <%= rand(999999) %></div></h>
<div class="panel">
 <form role="form">
<div class="row">
<div class="col-sm-12">
   <div class="well">
     <div class="radio-inline">
       <label><input type="radio" name="optradio">Área Urbana</label>
     </div>
     <div class="radio-inline">
      <label><input type="radio" name="optradio">Área Rural</label>
    </div>
  </div>
  </div>
</div>

<div class="row">
  <div class="col-sm-8">
    <label>
      Endereço
    </label>
    <input type="text" class="form-control">
  </div>

  <div class="col-sm-1">
    <label>
      Nº
    </label>
    <input type="text" class="form-control">
  </div>

  <div class="col-sm-3">
    <label>
      Complemento
    </label>
    <input type="text" class="form-control">
  </div>
</div>

<div class="row">
  <div class="col-sm-3">
    <label>
      Bairro
    </label>
    <input type="text" class="form-control">
  </div>

  <div class="col-sm-3">
    <label>
      Município
    </label>
    <input type="text" class="form-control">
  </div>

  <div class="col-sm-2">
    <label>
      CEP
    </label>
    <input type="text" class="form-control">
  </div>

  <div class="col-sm-1">
    <label>
      UF
    </label>
    <input type="text" class="form-control">
  </div>

  <div class="col-sm-1">
    <label>
      Área em M²
    </label>
    <input type="text" class="form-control">
  </div>

  <div class="col-sm-1">
    <label>
      Quadra
    </label>
    <input type="text" class="form-control">
  </div>

  <div class="col-sm-1">
    <label>
      Lote
    </label>
    <input type="text" class="form-control">
  </div>
</div>

<div class="row">
  <div class="col-sm-12">
    <label>
      Observação
    </label>
    <textarea rows="4" class="form-control textarea"></textarea>
  </div>
</div>

<div class="row">
  <div class="col-sm-12">
    <div class="well">
      <div class="sub_title">Dados do proprietário</div>
    </div>
  </div>
</div>

<div class="row">
  <div class="col-sm-9">
    <label>
      Nome do Proprietário
    </label>
    <input type="text" class="form-control">
  </div>

  <div class="col-sm-3">
    <label>
      CPF
    </label>
    <input type="text" class="form-control">
  </div>
</div>

<div class="row">
  <div class="col-sm-12">
    <label>
      Endereço de cobrança
    </label>
    <input type="text" class="form-control">
  </div>
</div>


<div class="row">
    <div class="col-sm-12">
      <div class="actions">
      <button class="btn btn-danger">Registrar Autuação</button>
        <button class="btn btn-primary">Registrar Notificação</button>
      </div>
    </div>
  </div>
</form>
</div>
